# Basic types
PORT=3000
API_KEY=your-api-key-here
DEBUG=true
LOG_LEVEL=info

# Number coercion
MAX_CONNECTIONS=100
TIMEOUT_MS=5000

# Boolean coercion
ENABLE_CACHE=true
ENABLE_METRICS=false

# Array coercion
ALLOWED_ORIGINS=https://example.com,https://app.example.com
TAGS=production,api,v1

# JSON coercion
DATABASE_CONFIG={"host":"localhost","port":5432}
FEATURE_FLAGS={"newUI":true,"betaFeatures":false}

# Optional values (can be omitted)
# OPTIONAL_VALUE=something
# CACHE_TTL=3600

# AWS Lambda Runtime Environment Variables
# These are automatically set by AWS Lambda at runtime
# Uncomment to simulate Lambda environment locally
# AWS_REGION=us-east-1
# AWS_LAMBDA_FUNCTION_NAME=my-lambda-function
# AWS_LAMBDA_FUNCTION_VERSION=$LATEST
# AWS_LAMBDA_FUNCTION_MEMORY_SIZE=512
# AWS_LAMBDA_LOG_GROUP_NAME=/aws/lambda/my-lambda-function
# AWS_LAMBDA_LOG_STREAM_NAME=2024/01/12/[$LATEST]abcdef1234567890
# AWS_EXECUTION_ENV=AWS_Lambda_nodejs20.x
# AWS_LAMBDA_RUNTIME_API=127.0.0.1:9001
# LAMBDA_TASK_ROOT=/var/task
# LAMBDA_RUNTIME_DIR=/var/runtime
