# =============================================================================
# Basic Types
# =============================================================================

# String
API_KEY=your-api-key-here
LOG_LEVEL=info

# Number (auto-coerced from string)
PORT=3000
MAX_CONNECTIONS=100

# Boolean (auto-coerced from string)
DEBUG=true

# Array (auto-split by comma)
ALLOWED_ORIGINS=https://example.com,https://app.example.com

# JSON (auto-parsed)
DATABASE_CONFIG={"host":"localhost","port":5432}

# =============================================================================
# AWS Parsed Types (NEW!)
# =============================================================================

# Validation-only types (return string)
BUCKET_NAME=my-bucket
TABLE_NAME=my-table
AWS_REGION=us-east-1

# Parsed types (return object with properties)
QUEUE_ARN=arn:aws:sqs:us-east-1:123456789012:my-queue
QUEUE_URL=https://sqs.us-east-1.amazonaws.com/123456789012/my-queue
TABLE_ARN=arn:aws:dynamodb:us-east-1:123456789012:table/my-table
S3_ARN=arn:aws:s3:::my-bucket/path/to/object.txt
ROLE_ARN=arn:aws:iam::123456789012:role/my-role

# =============================================================================
# AWS Lambda Runtime Environment Variables
# =============================================================================
# These are automatically set by AWS Lambda at runtime
# Uncomment to simulate Lambda environment locally

# AWS_REGION=us-east-1
# AWS_LAMBDA_FUNCTION_NAME=my-lambda-function
# AWS_LAMBDA_FUNCTION_VERSION=$LATEST
# AWS_LAMBDA_FUNCTION_MEMORY_SIZE=512
# AWS_LAMBDA_LOG_GROUP_NAME=/aws/lambda/my-lambda-function
# AWS_LAMBDA_LOG_STREAM_NAME=2024/01/12/[$LATEST]abcdef1234567890

